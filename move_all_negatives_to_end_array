#User function Template for python3

class Solution:
    def segregateElements(self, arr):
        l1=[]
        l2=[]
        
        for i in range (len(arr)):
            if arr[i]>=0:
                l1.append(arr[i])
            else:
                l2.append(arr[i])
        x=l1+l2
        for i in range(len(arr)):
            arr[i]=x[i]
        return arr